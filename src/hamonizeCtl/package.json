    "node-schedule": "^2.0.0",
    "ping": "^0.4.1",
    "pkg": "^5.2.1",
    "request": "^2.88.2",
    "save": "^2.4.0",
    "systeminformation": "^5.7.7",
    "tail": "^2.2.6",
    "unirest": "^0.6.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "jest": "^27.3.1",
    "util": "^0.12.4"
