{
  "name": "hamonizeCtl",
  "version": "1.0.0",
  "description": "A hamonizeCtl application",
  "main": "main.js",
  "url": "http://192.168.0.240:8083",
  "scripts": {
    "start": "sudo node main.js",
    "clean": "rm -rf node_modules/ ./hamonizeCtl",
    "build": "pkg . --out-path=dist",
    "test": "jest"
  },
  "bin": {
    "hamonizeCtl": "./main.js"
  },
  "pkg": {
    "scripts": "*.js",
    "assets": [
      "shell/**/*",
      "usb-lockdown**/*",
      "./node_modules/**/*"
    ],
    "targets": [
      "node14-linux-x64"
    ]
  },
  "repository": "https://github.com/hamonikr/hamonize",
  "keywords": [
    "hamonizeCtl"
  ],
  "author": "hamonikr",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-spinner": "^0.2.10",
    "commander": "^9.0.0",
    "date-utils": "^1.2.21",
    "diff": "^5.0.0",
    "figlet": "^1.5.2",
    "fs": "^0.0.2",
    "gradient-string": "^2.0.2",
    "https": "^1.0.0",
    "inquirer": "^8.2.5",
    "ip": "^1.1.5",
    "line-reader": "^0.4.0",
    "loading-cli": "^1.1.0",
